pipeline {
    agent any
    tools {
        maven 'Maven_3_9_11'
    }
    stages {
        stage('Checkout') {
            steps {
                checkout scm // Checkout code from SCM
            }
        }
        stage('Build') {
            steps {
                //sh 'mvn compile'
		sh -c "echo Hello from Jenkins!"
            }
        }
        stage('Test') {
            steps {
                //sh 'mvn test'
		sh "And this is a test stage"
            }
        }
    }
}
