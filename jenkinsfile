pipeline {
    agent any
    tools {
        maven 'Maven_3_9_11'
    }
    stages {
        stage('Checkout') {
            steps {
                checkout scm // Checkout code from SCM
            }
        }
        stage('Build') {
            steps {
                sh 'mvn compile'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}
